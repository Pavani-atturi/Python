#fundtion
def calc(n1,n2,s):
    if s=="+":
        print("the sum is:",n1+n2)
    elif s=="-":
        print("the difference is:",n1-n2)
    elif s=="*":
        print("the multiply is:",n1*n2)
    elif s=="/":
        print("the division is:",n1/n2)
        
# Input
n1=int(input("Enter number-1:"))
n2=int(input("Enter nyumber-2:"))
s=input("Enter the s(+,-,*,/):")

calc(n1,n2,s)
